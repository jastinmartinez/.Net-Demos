@model IEnumerable<CXP.Models.PayConcept>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Concepto De Pago</h2>

<p>
    @Html.ActionLink("Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PayConceptDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PayConceptState)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PayConceptDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PayConceptState)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.PayConceptID }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.PayConceptID }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.PayConceptID })
            </td>
        </tr>
    }

</table>
